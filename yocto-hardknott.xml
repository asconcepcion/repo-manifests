	<remote name="yocto"  fetch="https://git.yoctoproject.org/git"/>
	<remote name="openembedded"  fetch="git://git.openembedded.org/"/>
	<remote name="qt" fetch="git://code.qt.io/"/>
	<remote name="github" fetch="ssh://git@github.com"/>

	<project remote="yocto" name="poky"
				revision="refs/heads/hardknott"
				path="sources/poky"/>

	<project remote="yocto" name="meta-raspberrypi"
